<div id="<%= dom_id campu %>">
  <p>
    <strong>Descrição: </strong>
    <%= campu.description %>
  </p>

  <p>
    <strong>Endereço: </strong>
    <%= campu.adress %>
  </p>

  <p>
    <strong>Número: </strong>
    <%= campu.number %>
  </p>

  <p>
    <strong>Bairro: </strong>
    <%= campu.neighbourhood %>
  </p>

  <p>
    <strong>Cidade: </strong>
    <%= campu.city %>
  </p>

  <p>
    <strong>Cep:</strong>
    <%= campu.cep %>
  </p>

  <p>
    <strong>Data de desativação: </strong>
    <%= campu.deactivationDate %>
  </p>

</div>
